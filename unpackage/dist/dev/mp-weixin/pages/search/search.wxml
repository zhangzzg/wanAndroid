<view><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{2000}}" duration="{{1000}}"><block wx:for="{{images}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['imageClick',['$0'],[[['images','',index]]]]]]]}}" bindtap="__e"><view><image style="width:750rpx;" src="{{item.imagePath}}"></image></view></swiper-item></block></swiper><view><block wx:for="{{totalData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['actionDetail',['$0'],[[['totalData','',index]]]]]]]}}" class="list-item" bindtap="__e"><view class="list-title"><text hidden="{{!(item.type==1)}}" class="label">置顶</text><text hidden="{{!(item.fresh)}}" class="label">新</text><block wx:if="{{item.tags!=null&&item.tags.length>0}}"><text class="label" style="color:#4CD964;">{{item.tags[0].name}}</text></block><block wx:if="{{item.author.length>0}}"><text class="author">{{item.author}}</text></block><block wx:else><text class="author">{{item.shareUser}}</text></block><text class="time">{{item.niceDate}}</text></view><view class="list-content"><image hidden="{{!(item.envelopePic.length>0)}}" class="image" src="{{item.envelopePic}}" mode="aspectFill"></image><view class="info"><text class="title">{{item.title}}</text><view class="bottom"><text class="noted">{{item.superChapterName+"/"+item.chapterName}}</text><view data-event-opts="{{[['tap',[['favClick',['$event']]]]]}}" catchtap="__e"><uni-fav vue-id="{{'50cad900-1-'+index}}" checked="{{item.collect}}" bind:__l="__l"></uni-fav></view></view></view></view><view class="line"></view></view></block></view><uni-fab vue-id="50cad900-2" horizontal="right" vertical="bottom" data-event-opts="{{[['^fabClick',[['fabClick']]]]}}" bind:fabClick="__e" bind:__l="__l"></uni-fab></view>